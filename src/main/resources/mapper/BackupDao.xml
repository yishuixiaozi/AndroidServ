<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hhit.dao.BackupDao">
    <select id="querybackup" resultType="com.hhit.model.BackupBean">
        SELECT * from back
    </select>

    <!--数据库备份-->
    <insert id="addbackup" parameterType="com.hhit.model.BackupBean">
        INSERT into back(backupusername,backuptime,backupfilename,backupfilepath) VALUES (#{backupusername},
        #{backuptime},#{backupfilename},#{backupfilepath})
    </insert>
</mapper>